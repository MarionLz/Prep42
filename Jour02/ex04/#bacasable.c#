#include <unistd.h>

int ft_putchar(char c)
{
  write(1, &c, 1);

		  ft_putchar(x);
		  ft_putchar(y);
		  ft_putchar(z);
		  if (x != '7' || y != '8' || z != '9')
		    {
		      ft_putchar(',');
		      ft_putchar(' ');
		    }
		}
	      z = z + 1;
	    }
	  y = y + 1;
	}
      x = x + 1;
    }
}

int main()
{
  ft_print_comb();
  ft_putchar('\n');
  return (0);
}
